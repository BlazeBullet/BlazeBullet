{
  "executable": "BlazeBullet",
  "splashscreen": {
    "imageFile": "./wwwroot/favicon.png"
  },
  "name": "BlazeBullet",
  "author": "SirTenzin, Ruri, demiurgo and meinname",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "appId": "dev.OpenBullet.app",
    "productName": "BlazeBullet2",
    "copyright": "Copyright Â© 2021",
    "buildVersion": "0.1.2",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}